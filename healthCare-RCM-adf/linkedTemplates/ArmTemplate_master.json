{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "healthCare-RCM-adf"
		},
		"ls_healthCare_adb_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "0206-195714-n8epyf5g"
		},
		"ls_healthCare_adls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://healthcarearsa.dfs.core.windows.net/"
		},
		"ls_healthCare_kv_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://healthCare-RCM-AR-kv.vault.azure.net/"
		},
		"ls_healthCare_sql_db_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "healthcare-sql-server.database.windows.net"
		},
		"ls_healthCare_sql_db_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "@{linkedService().dbName}"
		},
		"ls_healthCare_sql_db_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "healthcare"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "healthCare-RCM-adf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_healthCare_kv_properties_typeProperties_baseUrl": {
						"value": "[parameters('ls_healthCare_kv_properties_typeProperties_baseUrl')]"
					},
					"ls_healthCare_adb_properties_typeProperties_existingClusterId": {
						"value": "[parameters('ls_healthCare_adb_properties_typeProperties_existingClusterId')]"
					},
					"ls_healthCare_adls_properties_typeProperties_url": {
						"value": "[parameters('ls_healthCare_adls_properties_typeProperties_url')]"
					},
					"ls_healthCare_sql_db_properties_typeProperties_server": {
						"value": "[parameters('ls_healthCare_sql_db_properties_typeProperties_server')]"
					},
					"ls_healthCare_sql_db_properties_typeProperties_database": {
						"value": "[parameters('ls_healthCare_sql_db_properties_typeProperties_database')]"
					},
					"ls_healthCare_sql_db_properties_typeProperties_userName": {
						"value": "[parameters('ls_healthCare_sql_db_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}